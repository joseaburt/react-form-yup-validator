  "bugs": {
    "url": "https://github.com/joseaburt/react-form-yup-validator/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react",
    "typescript",
    "form",
    "framework",
    "form-validation"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
