  "jest": {
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "yup": "^1.0.2"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/react-form-yup-validator.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-form-yup-validator.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "@types/yup": "^0.32.0",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "size-limit": "^11.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}